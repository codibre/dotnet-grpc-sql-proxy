#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/yaml@2.5.1/node_modules/yaml/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/yaml@2.5.1/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/yaml/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/yaml@2.5.1/node_modules/yaml/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/yaml@2.5.1/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/yaml/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../yaml/bin.mjs" "$@"
else
  exec node  "$basedir/../yaml/bin.mjs" "$@"
fi

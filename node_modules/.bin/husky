#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/husky@9.1.6/node_modules/husky/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/husky@9.1.6/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/husky/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/husky@9.1.6/node_modules/husky/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/husky@9.1.6/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/grpc-sql-proxy/node_modules/husky/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../husky/bin.js" "$@"
else
  exec node  "$basedir/../husky/bin.js" "$@"
fi
